<script>

var toggle_count = 0;

window.setInterval(function() {
  
  if (toggle_count==0) {
    $("#toggle3").fadeOut(600)
    $("#toggle1").delay(650).fadeIn()
  } else if (toggle_count==1) {
    $("#toggle1").fadeOut(600)
    $("#toggle2").delay(650).fadeIn()
  } else {
    $("#toggle2").fadeOut(600)
    $("#toggle3").delay(650).fadeIn()
  }
  toggle_count++;
  toggle_count = toggle_count%3;
}, 5000);
</script>