<div id="toggle<%=panel_id%>">
  <%=timeframe%>
  <div class="row no-gutter">
    <div class="col-sm-4"> 
        <h2>Number of calls made</h2>
          <div id="calls-made-<%=timeframe%>"> 
          </div>
    </div><!-- col-sm-4 --> 
    <div class="col-sm-4">
      <h2>Deals Won</h2>
        <div id="won-<%=timeframe%>">
          <%= render partial: 'dashboard/won', locals: {timeframe: timeframe } %>
        </div><!---won-<%=timeframe%>-->  
    </div><!-- col-md-4 -->
    <div class="col-sm-4"> 
      <h2>Average calls per day</h2>
        <div id="calls-day-<%=timeframe%>"> 
        </div>
    </div><!-- col-sm-4 --> 
  </div><!-- row --> 
<div class="row no-gutter">
  <div class="col-sm-4">
    <h2>Revenue</h2>
    <div id="revenue-<%=timeframe%>">
    </div><!--revenue-->
  </div><!-- col-md-4 -->
  <div class="col-sm-4">
    <h2>Call Conversion Rate</h2>
    <div id="ccr-<%=timeframe%>">
      <%= render partial: 'dashboard/ccr', locals: {timeframe: timeframe } %>
    </div><!-- ccr --> 
  </div><!-- col-sm-4 -->
  <div class="col-sm-4">
    <h3>Average Budget per Campaign</h3>
    <div id="budget-<%=timeframe%>">
    </div><!-- budget -->
  </div><!-- col-md-4 -->
</div><!-- row -->
<div class="row no-gutter">
  <div class="col-sm-3">
    <p class="a">Person A</p>
  </div><!-- col-sm-3 -->
  <div class="col-sm-3">
    <p class="b">Person B</p>
   </div><!-- col-sm-3 -->
  <div class="col-sm-3">
  <p class="c">Person C</p>
  </div><!-- col-sm-3 -->
  <div class="col-sm-3">
  <p class="d">Person D</p>
  </div><!-- col-sm-3 -->
</div><!-- row no-gutter -->
</div> <!--toggle-1-->